{
  "name": "presto",
  "version": "1.0.0",
  "description": "A webpack boilerplate for building wordpress themes",
  "author": "eddo81 <eduardo_jonnerstig@live.com>",
  "private": true,
  "scripts": {
    "start": "npm run watch & npm run serve & wait",
    "watch": "cross-env NODE_ENV=development node build/tools/build.js",
    "serve": "cross-env NODE_ENV=development node build/tools/serve.js",
    "build": "cross-env NODE_ENV=production node build/tools/build.js",
    "lint": "eslint --ext .js build"
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^8.5.2",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "browser-sync": "^2.24.4",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "es6-promise": "^4.2.4",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eventsource-polyfill": "^0.9.6",
